{
  "name": "cuby-orm",
  "version": "0.0.3",
  "description": "Cuby-ORM is an Object Relational Mapping (ORM) library for Node.js applications. It simplifies database interaction by allowing developers to scan existing database models and create new ones. Additionally, Cuby-ORM supports migrations and seeders, making it easy to manage database changes and populate initial data.",
  "main": "build/index.js",
  "types": "build/types/index.d.ts",
  "bin": {
    "cuby": "./build/bin/index.js"
  },
  "scripts": {
    "build": "rm -rf build  && npx tsc  -p tsconfig.prod.json",
    "watch:dev": "rm -rf build && tsc -p tsconfig.dev.json -w",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-package-npm/cuby-orm.git"
  },
  "keywords": [
    "orm",
    "mysql",
    "cli",
    "cuby-orm",
    "databases",
    "migration",
    "seeds",
    "models"
  ],
  "author": "Jairo Rohatan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dev-package-npm/cuby-orm/issues"
  },
  "homepage": "https://github.com/dev-package-npm/cuby-orm#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "inquirer": "^8.2.4",
    "pg": "^8.10.0",
    "promise-mysql": "^5.2.0",
    "spinnies": "^0.5.1",
    "ts-code-generator": "^0.0.3",
    "ts-mixer": "^6.0.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/pg": "^8.6.6",
    "@types/spinnies": "^0.5.0",
    "typescript": "^4.9.5"
  }
}